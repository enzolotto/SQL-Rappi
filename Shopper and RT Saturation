(SELECT 
DISTINCT CO.STORE_ID, CO.ORDER_ID, OS.PHYSICAL_STORE_ID, CO.STORE_NAME, ov.created_at , ov.name, ROUND(OV.PARAMS:rt_saturation,2) AS RTs_SATURATION, ROUND(OV.PARAMS:sh_saturation,2) AS PS_SATURATION
FROM BR_PG_MS_CPGOPS_EVENTS_MS_PUBLIC.ORDER_EVENTS OV
JOIN CO_WRITABLE.GORTALLORDERSMASTER CO ON CO.ORDER_ID = OV.ORDER_ID --AND co.STORE_NAME ILIKE ('Carrefour Hiper - Lim√£o (SPL)') 
JOIN BR_PG_MS_CPGOPS_ORDERS_MS_PUBLIC.ORDER_SUMMARY OS ON OS.ORDER_ID = OV.ORDER_ID
WHERE os.PHYSICAL_STORE_ID=50
AND DATEDIFF(WEEK, CO.DATE,CURRENT_DATE) < 3
AND
OV.name IN (
'order_to_sh',
'order_to_rt',
'to_change_was_taken_order_to_sh')
ORDER BY 5 DESC
)
